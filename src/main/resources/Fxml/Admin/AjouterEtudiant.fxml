<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="750.0" styleClass="ajouter_etudiant_container" stylesheets="@../../Styles/AjouterEtudiant.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gestionetudiant.projet.Controllers.Admin.AjouterEtudiantController">
    <VBox fillWidth="false" layoutX="14.0" layoutY="4.0" prefHeight="539.0" prefWidth="601.0">
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nom:" />
        <TextField fx:id="nom_fld" />
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Prenom:" />
        <TextField fx:id="prenom_fld" />
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Date de naissance:" />
        <TextField fx:id="date_naissance_fld" />
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Specialité:" />
        <TextField fx:id="specialite_fld" />
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Mot de pass:" />
        <TextField fx:id="mot_pass_fld" />
       <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Id Utilisateur" />
        <TextField fx:id="id_utilisateur_fld" />
        <Button fx:id="ajouter_etudiant_btn" mnemonicParsing="false" text="Ajouter nouveau étudiant" />
        <!--<Label fx:id="error_lbl" styleClass="error_lbl" text="Error" />-->
    </VBox>
</AnchorPane>
